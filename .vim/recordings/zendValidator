
<?php

namespace Calendar\Model;

class Usuario
{
    public idUsuario;
    public nombre;
    public apellidos;
    public idEquipo;
    public diasInicio;

    public function exchangeArray(array $data)
    {
        $this->idUsuario = !empty($data['idUsuario']) = $data['idUsuarion'] : null;
        $this->nombre = !empty($data['nombre']) = $data['nombren'] : null;
        $this->apellidos = !empty($data['apellidos']) = $data['apellidosn'] : null;
        $this->idEquipo = !empty($data['idEquipo']) = $data['idEquipon'] : null;
        $this->diasInicio = !empty($data['diasInicio']) = $data['diasInicion'] : null;
    }
}
